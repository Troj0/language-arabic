{
  "name": "alterbyte/language-arabic",
  "type": "flarum-extension",
  "description": "Arabic Language Pack.",
  "keywords": ["locale", "Arabic", "العربية"],
  "license": "MIT",
  "require": {
      "flarum/core": "^0.1.0-beta.13"
  },
  "autoload": {
    "psr-4": {
      "Alterbyte\\LanguageArabic\\": "src/"
    }
  },
  "extra": {
      "flarum-extension": {
          "title": "Arabic Language",
          "icon": {
              "image": "icon.svg",
              "backgroundColor": "#006233",
              "backgroundSize": "cover",
              "backgroundPosition": "center"
          }
      },
      "flarum-locale": {
          "code": "ar",
          "title": "Arabic"
      }
  }
}
